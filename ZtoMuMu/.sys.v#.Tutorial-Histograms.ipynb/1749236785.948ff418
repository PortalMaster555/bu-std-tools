   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "id": "3c3689e7",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "<cppyy.gbl.TCanvas object at 0x11104700>"
      ]
     },
     "execution_count": 34,
     "metadata": {},
     "output_type": "execute_result"
    },
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "Warning in <TCanvas::Constructor>: Deleting canvas with same name: c1\n"
     ]
    }
   ],
   "source": [
    "## Init the canvas\n",
    "\n",
    "c1 = ROOT.TCanvas('c1', '', 600, 500)\n",
    "c1.cd()"
   ]
  },
  {
